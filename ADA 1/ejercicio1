with Text_IO;

procedure Main is

	procedure ejercicio1 is
		variable:Natural;
		package Natural_IO is new Text_IO.INteger_IO(Natural);
	begin
		Text_IO.Put_Line("---------Ejercicio 1---------");
		Text_IO.Put("valor: ");
		Text_IO.Get(variable);
		Text_IO.Put_Line("------------------------");

	end ejercicio1;
	begin
		--Insert code here
		null;
	end Main;
